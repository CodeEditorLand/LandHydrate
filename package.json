{
  "name": "vscode-hydrate",
  "displayName": "VSCode Hydrate",
  "version": "1.3.1",
  "description": "Hydrate for VSCode Kubernetes Tools",
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/vscode-hydrate.git"
  },
  "license": "MIT",
  "publisher": "madelineliao",
  "main": "./out/extension.js",
  "scripts": {
    "compile": "tsc -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "vscode:prepublish": "npm run compile",
    "watch": "tsc -watch -p ./"
  },
  "contributes": {
    "commands": [
      {
        "command": "vshydrate.hydrateCluster",
        "title": "Hydrate Cluster"
      }
    ],
    "menus": {
      "view/item/context": [
        {
          "command": "vshydrate.hydrateCluster",
          "group": "hydrate",
          "when": "view == extension.vsKubernetesExplorer && viewItem =~ /vsKubernetes\\.\\w*cluster(?!.*\\.inactive$).*/i"
        }
      ]
    }
  },
  "activationEvents": [
    "onView:extension.vsKubernetesExplorer",
    "onCommand:vshydrate.hydrateCluster"
  ],
  "dependencies": {
    "vscode-kubernetes-tools-api": "^1.0.0"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "^8.10.0",
    "@types/sinon": "^7.0.13",
    "glob": "^7.1.4",
    "mocha": "^6.1.4",
    "sinon": "^7.3.2",
    "typescript": "^3.5.1",
    "vscode": "^1.1.36"
  },
  "extensionDependencies": [
    "ms-kubernetes-tools.vscode-kubernetes-tools"
  ],
  "preview": true
}
