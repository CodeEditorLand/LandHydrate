{
	"activationEvents": [
		"onView:extension.vsKubernetesExplorer",
		"onCommand:vshydrate.hydrateCluster"
	],
	"contributes": {
		"commands": [
			{
				"command": "vshydrate.hydrateCluster",
				"title": "Hydrate Cluster"
			}
		],
		"menus": {
			"view/item/context": [
				{
					"command": "vshydrate.hydrateCluster",
					"group": "hydrate",
					"when": "view == extension.vsKubernetesExplorer && viewItem =~ /vsKubernetes\\.\\w*cluster(?!.*\\.inactive$).*/i"
				}
			]
		}
	},
	"dependencies": {
		"vscode-kubernetes-tools-api": "1.3.0"
	},
	"description": "Hydrate for VSCode Kubernetes Tools",
	"devDependencies": {
		"@types/glob": "8.1.0",
		"@types/mocha": "10.0.6",
		"@types/node": "20.11.30",
		"@types/sinon": "17.0.3",
		"glob": "10.3.10",
		"mocha": "10.3.0",
		"sinon": "17.0.1",
		"typescript": "5.4.2",
		"typescript-esbuild": "0.4.5",
		"vscode": "1.1.36"
	},
	"displayName": "VSCode Hydrate",
	"extensionDependencies": [
		"ms-kubernetes-tools.vscode-kubernetes-tools"
	],
	"main": "./out/extension.js",
	"name": "vscode-hydrate",
	"preview": true,
	"scripts": {
		"compile": "tsc -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'",
		"vscode:prepublish": "npm run compile",
		"watch": "tsc -watch -p ./"
	},
	"type": "module"
}
