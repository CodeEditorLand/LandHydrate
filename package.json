{
	"name": "vscode-hydrate",
	"displayName": "VSCode Hydrate",
	"version": "0.0.1",
	"private": false,
	"description": "Hydrate for VSCode Kubernetes Tools",
	"keywords": [
		"land"
	],
	"homepage": "HTTPS://GitHub.Com/CodeEditorLand/LandHydrate#readme",
	"bugs": {
		"url": "HTTPS://GitHub.Com/CodeEditorLand/LandHydrate/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+HTTPS://github.com/CodeEditorLand/LandHydrate.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "üåÜ‚ÄÅ‚Äî‚ÄÅLand‚ÄÅ‚Äî",
		"email": "Land@PlayForm.Cloud",
		"url": "HTTPS://Land.PlayForm.Cloud"
	},
	"type": "module",
	"main": "./out/extension.js",
	"scripts": {
		"Document": "Document 'Source/**/*.ts'",
		"compile": "tsc -p ./",
		"prepublishOnly": "Build 'Source/**/*.ts'"
	},
	"contributes": {
		"commands": [
			{
				"command": "vshydrate.hydrateCluster",
				"title": "Hydrate Cluster"
			}
		],
		"menus": {
			"view/item/context": [
				{
					"command": "vshydrate.hydrateCluster",
					"group": "hydrate",
					"when": "view == extension.vsKubernetesExplorer && viewItem =~ /vsKubernetes\\.\\w*cluster(?!.*\\.inactive$).*/i"
				}
			]
		}
	},
	"activationEvents": [
		"onView:extension.vsKubernetesExplorer",
		"onCommand:vshydrate.hydrateCluster"
	],
	"dependencies": {},
	"devDependencies": {
		"@playform/build": "0.0.7",
		"@playform/document": "0.0.6",
		"@types/glob": "^7.1.1",
		"@types/node": "^8.10.0",
		"@types/sinon": "^7.0.13",
		"glob": "^7.1.4",
		"sinon": "^7.3.2"
	},
	"extensionDependencies": [
		"ms-kubernetes-tools.vscode-kubernetes-tools"
	],
	"publishConfig": {
		"access": "public"
	},
	"preview": true
}
