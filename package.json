{
	"name": "vscode-hydrate",
	"displayName": "VSCode Hydrate",
	"description": "Hydrate for VSCode Kubernetes Tools",
	"license": "MIT",
	"publisher": "madelineliao",
	"version": "1.3.1",
	"repository": {
		"type": "git",
		"url": "https://github.com/microsoft/vscode-hydrate.git"
	},
	"engines": {
		"vscode": "1.31.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"bedrock",
		"fabrikate",
		"hydrate",
		"kubernetes",
		"k8s"
	],
	"preview": true,
	"activationEvents": [
		"onView:extension.vsKubernetesExplorer",
		"onCommand:vshydrate.hydrateCluster"
	],
	"extensionDependencies": [
		"ms-kubernetes-tools.vscode-kubernetes-tools"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "vshydrate.hydrateCluster",
				"title": "Hydrate Cluster"
			}
		],
		"menus": {
			"view/item/context": [
				{
					"command": "vshydrate.hydrateCluster",
					"when": "view == extension.vsKubernetesExplorer && viewItem =~ /vsKubernetes\\.\\w*cluster(?!.*\\.inactive$).*/i",
					"group": "hydrate"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "tslint -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"pretest": "npm run compile && npm run lint",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/glob": "8.1.0",
		"@types/mocha": "10.0.6",
		"@types/node": "20.11.28",
		"@types/sinon": "17.0.3",
		"glob": "10.3.10",
		"mocha": "10.3.0",
		"sinon": "17.0.1",
		"tslint": "5.20.1",
		"typescript": "5.4.2",
		"vscode": "1.1.36"
	},
	"dependencies": {
		"vscode-kubernetes-tools-api": "1.3.0"
	}
}
